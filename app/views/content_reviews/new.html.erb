<h1>投稿フォーム</h1>
  <%= form_with model: @content_review do |f| %>
    <h4>タイトル</h4>
      <%= f.text_field :title %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>入手ルート</h4>
      <%= f.text_field :get_root %>
    <h4>購入金額</h4>
      <%= f.text_field :price %>
    <h4>説明文</h4>
      <%= f.text_area :explanation %>
    <%= f.submit '投稿' %>
  <% end %>